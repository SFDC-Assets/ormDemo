    <decisions>
        <name>Determine_Tiers</name>
        <label>Determine Tiers</label>
        <locationX>602</locationX>
        <locationY>189</locationY>
        <defaultConnector>
            <targetReference>Tier_3_Results</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Tier 3</defaultConnectorLabel>
        <rules>
            <name>Tier_1</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Economic</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Lessthan250K</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Economic</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>$250Ktolessthan$1MM</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Economic</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>$1MMtolessthan$5MM</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Economic</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>$$5MM to &lt;$10MM</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Tier_1_Results</targetReference>
            </connector>
            <label>Tier 1</label>
        </rules>
    </decisions>
