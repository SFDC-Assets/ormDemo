    <formulas>
        <name>f_biaTier</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!EconomicPickListChoiceSet}, &quot;Less than 250K&quot;), &quot;Tier 1 - Aligned&quot;, &quot;Tier 3 - Aligned&quot;)</expression>
    </formulas>
    <formulas>
        <name>Lessthan250k</name>
        <dataType>String</dataType>
        <expression>IF(
{!Economic}=&quot;Less than $250K&quot;, &quot;Tier 1 - Aligned&quot;, &quot;Tier 3 - Aligned&quot;
)</expression>
    </formulas>
